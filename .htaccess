RewriteEngine On
RewriteRule ^$ /frontend/index.html [L]
RewriteRule ^index\.html$ /frontend/index.html [L,R=301]
RewriteRule ^index\.php$ /frontend/index.html [L,R=301]
RewriteCond %{REQUEST_URI} !^/(backend|api|frontend)
RewriteRule ^(.*)$ /frontend/$1 [L]

<FilesMatch "\.(html|css|js|png|jpg|jpeg|gif|ico)$">
    Header set Cache-Control "public, max-age=86400"
</FilesMatch>